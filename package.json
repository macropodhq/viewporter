{
  "name": "viewporter",
  "version": "1.0.0",
  "description": "Tools for working with all sorts of browser viewports",
  "keywords": [
    "viewport",
    "scaling",
    "position",
    "mobile"
  ],
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.4.7",
    "css-loader": "^0.15.6",
    "eslint": "^0.21.2",
    "file-loader": "^0.8.4",
    "jest-cli": "^0.4.18",
    "postcss-loader": "^0.5.1",
    "postcss-math": "0.0.1",
    "postcss-px-to-em": "^1.0.0",
    "postcss-simple-vars": "^0.3.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "author": "Jessica Stokes <jessica.stokes@macropod.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "start": "viewporter-start",
    "lint": "eslint --ext .js src/*.js",
    "prepublish": "babel src --out-dir lib --ignore __tests__ --source-maps inline"
  },
  "bin": {
    "viewporter-start": "./bin/start.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macropodhq/viewporter.git"
  },
  "jest": {
    "rootDir": "src",
    "scriptPreprocessor": "../node_modules/babel-jest",
    "testFileExtensions": [
      "js"
